{
  "name": "@rubemfsv/clean-react-app",
  "author": "rubemfsv",
  "version": "1.1.4",
  "description": "Clean React App",
  "main": "index.js",
  "keywords": [
    "React",
    "Clean Architecture",
    "Typescript",
    "Webpack",
    "Clean React App"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "start": "node index",
    "dev:base": "webpack-dev-server --config webpack.dev.js",
    "dev": "npm run dev:base -- --open",
    "build": "webpack --config webpack.prod.js",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "npm test -- --watch",
    "test:e2e": "node_modules/.bin/cypress open",
    "postinstall": "npx husky install",
    "format": "prettier --check --no-error-on-unmatched-pattern --ignore-path .gitignore . --ext ts,tsx,js,jsx,json,md",
    "format:fix": "prettier --write --no-error-on-unmatched-pattern --ignore-path .gitignore . --ext ts,tsx,js,jsx,json,md"
  },
  "bin": "./src/main/scripts/bin.js",
  "repository": "https://github.com/rubemfsv/clean-react-app.git",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rubemfsv/clean-react-app/issues"
  },
  "homepage": "https://github.com/rubemfsv/clean-react-app#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/faker": "^5.1.5",
    "@types/jest": "^29.5.5",
    "@types/node": "^14.14.17",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.1.7",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "cypress": "^13.2.0",
    "express": "^4.17.1",
    "express-history-api-fallback": "^2.2.1",
    "faker": "^5.1.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.2",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-localstorage-mock": "^2.4.6",
    "mini-css-extract-plugin": "^1.3.4",
    "netlify-webpack-plugin": "^1.1.1",
    "node-sass": "^8.0.0",
    "prettier": "3.0.3",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^8.0.13",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.1.0",
    "react-router-dom": "^5.2.0"
  }
}
